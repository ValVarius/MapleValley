<div>
  {{> navbar/navbar}}
</div>
</br>
<div class="columns">
  <div class="column is-one"></div>
  <div class="column is-10">
    <div class="box">
      <h1 class="title">Your registrations for Current Events:</h1>
      <a href="/role" class="button" type="submit">Register more people for SummerCamp!</a>
      </br>
      {{#each data}}
      </br>
      <div class="card">
        <div class="card-content">
          <div class="media">
            <div class="media-content">
              <p class="title">{{first_name}} {{last_name}}</p>
              <p class="subtitle">{{email}}</p>
            </div>
          </div>
          <div class="content">
            <p class="subtitle">Unit Number: {{service_unit_number}}</p>
            <p class="subtitle">Troop Number: {{troop_number}}</p>
            <p class="subtitle">Authorized Names: {{authorized_release_names}}</p>
            <time>Registration Date: {{createdAt}}</time>
            </br>
            <a href="/camper/{{id}}" class="button is-primary">View Info</a>
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
  <div class="column is-one"></div>
</div>